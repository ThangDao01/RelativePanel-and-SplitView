<Page
    x:Class="Working_with_Navigation.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Working_with_Navigation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button Margin="0,0,20,0" Content="Home" Click="Home_Button" />
                <Button Margin="0,0,20,0" Click="Back_Button" Content="Back"/>
                <Button Margin="0,0,20,0" Click="Forward" Content="Forward"/>
            </StackPanel>
            <Frame Name="MyFrame">
                
            </Frame>
         

        </StackPanel>
     
    </Grid>
</Page>
